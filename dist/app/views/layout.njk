<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    {% include "parts/styles.njk" %}

    {% block head %}{% endblock %}

  </head>
  <body>
      {# INCLUI MENSAGEM DE ALERTAS #}
      {% include "parts/messages.njk" %}

      {# CONTÉM POSIÇÃO FIXED RELATIVA AO BODY #}
      <header>
        <div class="header_content">
          <h1>BINGÃO DA QUINA</h1>
        </div>
      </header>

      {# MENU MOBILE #}
      {# CONTÉM POSIÇÃO FIXED RELATIVA AO BODY #}
      <button class="btn_mob_menu toggle_menu">
        <i class="material-icons">menu</i>
      </button>

      {# MENU OVERLAY #}
      {# CONTÉM POSIÇÃO FIXED RELATIVA AO BODY #}
      <div class="menu_overlay toggle_menu"></div>


      <div class="mob_menu default_menu btn_mob_menu_is_closed">
        {% include "parts/box_avatar.njk" %}
        <hr>

        {% include "parts/menu.njk" %}

        {% include "parts/menu_admin.njk" %}

      </div>

      <div id="content">
        <section id="sidebar">
          {% include "parts/box_avatar.njk" %}

          <div class="desk_menu default_menu">
            <h2>MENU</h2>
            {% include "parts/menu.njk" %}

          </div>

          <div class="desk_menu default_menu">
            {% include "parts/menu_admin.njk" %}
          </div>

        </section>

        <section id="pages">

          {% block title %}{% endblock %}

          <div class="box_content">

            {% block content %}{% endblock %}

          </div>

          {# <div style="border: solid 1px red; height:1330px">Segunda DIV</div> #}

        </section>
      </div>

      {% include "parts/footer.njk" %}

  </body>
</html>
