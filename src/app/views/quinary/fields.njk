<label for="name">NÚMERO DO CONCURSO</label>
<div class="imput_container limit_container">
  <i class="material-icons">person</i>
  <input
    type="text"
    name="contest"
    required
    {% if quinaryContest %}
      value="{{ quinaryContest.contest  }}"
    {% else %}}
      value="{{ quinary.contest  }}"
    {% endif %}}
    placeholder="Digite o número do concurso"
    readonly
  >
  <span class="error"></span>
</div>

<div class="informative">
  <div class="info">
    <span class="material-icons">info</span>
    Campo preenchido automaticamente!
  </div>
</div>

<label for="name">DATA DO CONCURSO</label>
<div class="imput_container limit_container">
  <i class="material-icons">event</i>
  <input
    {# type="text" #}
    type="date"
    name="contestdata"
    required
    maxlength="8"
    min="2020-08-01"
    max="2040-01-01"
    pattern="/^(((0[1-9]|[12][0-9]|3[01])([-./])(0[13578]|10|12)([-./])(\d{4}))|(([0][1-9]|[12][0-9]|30)([-./])(0[469]|11)([-./])(\d{4}))|((0[1-9]|1[0-9]|2[0-8])([-./])(02)([-./])(\d{4}))|((29)(\.|-|\/)(02)([-./])([02468][048]00))|((29)([-./])(02)([-./])([13579][26]00))|((29)([-./])(02)([-./])([0-9][0-9][0][48]))|((29)([-./])(02)([-./])([0-9][0-9][2468][048]))|((29)([-./])(02)([-./])([0-9][0-9][13579][26])))$/"
    value="{{ quinary.contestdata }}"
    placeholder="Ex: 00/00/0000"
  >
  <span class="error"></span>
</div>

<label for="name">1ª DEZENA</label>
<div class="imput_container limit_container">
  <i class="material-icons">person</i>
  <input
    type="number"
    name="ten_first"
    required
    min="01"
    max="80"
    value="{{ quinary.ten_first}}"
    placeholder="Digite a 1ª dezena"
  >
