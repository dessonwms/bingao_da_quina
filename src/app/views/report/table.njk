<div class="tab_table">
  <h2>LISTA DE GANHADORES</h2>
  <table class="winner_result"
    data-noduplicates={{noDuplicates}}
  >
    <tr>
      <th>JOGADOR</th>
      <th colspan="10">NÃšMEROS</th>
      <th>ACERTOS</th>
    </tr>
    <tr>

    {% for onlyWinner in onlyWinners %}
      <tr>
        <td>{{onlyWinner.name}}</td>
        <td class="number">{{onlyWinner.first_ten}}</td>
        <td class="number">{{onlyWinner.second_ten}}</td>
        <td class="number">{{onlyWinner.third_ten}}</td>
        <td class="number">{{onlyWinner.forth_ten}}</td>
        <td class="number">{{onlyWinner.fifth_ten}}</td>
        <td class="number">{{onlyWinner.sixth_ten}}</td>
        <td class="number">{{onlyWinner.seventh_ten}}</td>
        <td class="number">{{onlyWinner.eighth_ten}}</td>
        <td class="number">{{onlyWinner.ninth_ten}}</td>
        <td class="number">{{onlyWinner.tenth_ten}}</td>
        <td {% if onlyWinner.number_hits == 10 %} class="minor_hit" {% endif %}>
          {{onlyWinner.number_hits}}
          {% if onlyWinner.number_hits == onlyWinner.minimo %}
            <span class="minor_hit">( menor acerto )</span>
          {% endif %}
        </td>
      </tr>
    {% endfor %}

  </table>
</div>
