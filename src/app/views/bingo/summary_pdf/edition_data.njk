<div class="tab_table">
    <h2 style="text-align: center">{{globalInfo.nameApp}} - {{bingo.edition}}ª EDIÇÃO</h2>
    <table>
      <tr>
        <th colspan="3">VALORES DAS PREMIAÇÕES</th>
        <th colspan="2">QTD DE PARTICIPANTES: </th>
        {# <th>{{ratings[0].totalbets}}</th> #}
      </tr>
      <tr>
        <th>ACERTOS</th>
        <th>QTD GANHADORES</th>
        <th>POR GANHADOR</th>
        <th>TOTAL <br> PREMIAÇÕES</th>
        <th>%</th>
        {# <th>(%) TOTAL</th> #}
      </tr>

      {% for ranking in dataSummary %}
        <tr
          {% if ranking.number_hits == 10 %}
            class="detach"
          {% elif ranking.number_hits == 9 %}
            class="detach"
          {% elif ranking.number_hits == ranking.minimo %}
            class="detach"
          {% endif %}
        >
          {% if ranking.number_hits == ranking.minimo %}
            <td><span class="detach_small">( MENOR ACERTO )</span><br><span class="detach">DEZENA: 2</span></td>
          {% else %}
            <td>{{ranking.number_hits}}</td>
          {% endif %}

          <td>{{ranking.count}}</td>

          {% if ranking.number_hits == 10 %}
            <td>{{summary.prizeForTenHits}}</td>
          {% elif ranking.number_hits == 9 %}
            <td>{{summary.prizeForNineHits}}</td>
          {% elif ranking.number_hits == ranking.minimo %}
            <td>{{summary.prizeMinorHit}}</td>
          {% else %}
            <td>--</td>
          {% endif %}

          {% if ranking.number_hits == 10 %}
            <td>{{summary.firstPlaceAward}}</td>
          {% elif ranking.number_hits == 9 %}
            <td>{{summary.secondPlaceAward}}</td>
          {% elif ranking.number_hits == ranking.minimo %}
            <td>{{summary.minorHitAward}}</td>
          {% else %}
            <td>--</td>
          {% endif %}

          {% if ranking.number_hits == 10 %}
            <td>80%</td>
          {% elif ranking.number_hits == 9 %}
            <td>10%</td>
          {% elif ranking.number_hits == ranking.minimo %}
            <td>10%</td>
          {% else %}
            <td>0%</td>
          {% endif %}

          {# {% if ranking.number_hits == 10 %}<td rowspan="{{totalRatings}}">80%</td> {% endif %} #}
        </tr>
      {% endfor %}

      {# <tr class="adm_fee format_font">
        <td colspan="3" class="align_right">TAXA DE ADMINISTRAÇÃO</td>
        <td>{{summary.administrationFee}}</td>
        <td></td>
        <td>20%</td>
      </tr> #}
      <tr class="total_collection format_font">
        <td colspan="3">TOTAL</td>
        <td>{{summary.prizeTotal}}</td>
        <td></td>
        {# <td></td> #}
      </tr>
      <tr class="value_bet format_font">
        <td colspan="3">VALOR DA APOSTA</td>
        <td>{{summary.valueBets}}</td>
        <td></td>
        {# <td></td> #}
      </tr>

    </table>
  </div>
